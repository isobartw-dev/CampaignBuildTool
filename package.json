{
	"name": "CampaignBuildTool",
	"version": "2.0.0",
	"description": "ISOBAR TW frontend campaign build tool",
	"dependencies": {
		"winreg": "1.2.2",
		"@csstools/postcss-sass": "latest",
		"autoprefixer": "^8.2.0",
		"browser-sync": "^2.23.6",
		"cssnano": "latest",
		"dev-ip": "1.0.1",
		"imagemin": "5.3.1",
		"imagemin-gifsicle": "5.2.0",
		"imagemin-jpeg-recompress": "5.1.0",
		"imagemin-pngquant": "5.0.1",
		"imagemin-svgo": "5.2.2",
		"images": "3.0.1",
		"ngrok": "^2.3.0",
		"onchange": "^3.2.1",
		"parallelshell": "latest",
		"postcss": "6.0.20",
		"postcss-scss": "1.0.4",
		"postcss-sprites": "4.2.1",
		"tiny-sprintf": "latest"
	},
	"scripts": {
		"prepublish": "node task/init.js",
		"runIIS": "node task/runIIS.js",
		"dev": "parallelshell \"npm run watch:all\" \"node task/browsersync.js\"",
		"watch:css": "onchange \"**/*.css\" -e \"**/css/style.css\" -o \"echo {{changed}} > task/css.txt && node  task/csscompiled.js\"",
		"watch:image": "onchange \"E:/Snow.Huang/My documents/Desktop/Output/**/*\" -e *.jpg -- node task/imagefolder.js",
		"watch:all": "parallelshell \"npm run watch:image\" \"npm run watch:css\"",
		"deploy:css": "node task/cssmin.js",
		"deploy:image": "node task/imagemin.js",
		"deploy:css+image": "del /s npm-debug.log.* && npm run deploy:css && npm run deploy:image",
		"deploy:all": "del /s npm-debug.log.* && npm run deploy:webfont && npm run deploy:css && npm run deploy:image"
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/isobartw-dev/CampaignBuildTool.git"
	},
	"author": "ISOBAR_TW-frontend",
	"bugs": {
		"url": "https://github.com/isobartw-dev/CampaignBuildTool.git/issues"
	},
	"homepage": "https://github.com/isobartw-dev/CampaignBuildTool.git#readme"
}
