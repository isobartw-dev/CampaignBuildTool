{
	"name": "project-dev-tool",
	"version": "2.0.0",
	"description": "ISOBAR TW frontend project development tool",
	"dependencies": {
		"autoprefixer": "^8.2.0",
		"browser-sync": "^2.23.6",
		"concurrently": "^4.1.0",
		"cssnano": "latest",
		"dev-ip": "1.0.1",
		"imagemin": "5.3.1",
		"imagemin-gifsicle": "5.2.0",
		"imagemin-jpeg-recompress": "5.1.0",
		"imagemin-pngquant": "5.0.1",
		"imagemin-svgo": "5.2.2",
		"ngrok": "^2.3.0",
		"node-sass": "^4.11.0",
		"nodemon": "^1.18.9",
		"onchange": "^5.2.0",
		"postcss": "^7.0",
		"postcss-sprites": "4.2.1",
		"postcss-url": "^8.0.0",
		"sharp": "^0.21.0",
		"tiny-sprintf": "latest",
		"winreg": "1.2.2"
	},
	"scripts": {
		"prepublish": "node task/init.js",
		"runIIS": "node task/runIIS.js",
		"dev": "concurrently \"npm run watch:css\" \"node task/browsersync.js\"",
		"watch:css:nodemon": "node task/watch-css.js",
		"watch:image:nodemon": "node task/watch-image.js",
		"watch:css": "onchange -v --await-write-finish -k -f \"change\" -o \"> task/.changelog && node task/csscompiled.js\" \"**/{css,css/*}/*.{scss,css}\" -e \"**/{css,*/css}/{style,style-source,style-edit}.css\" -- echo {{changed}}",
		"watch:image": "onchange --await-write-finish -k -f \"add change\" -o \"> task/.changelog && node task/imagefolder.js\" \"E:/Snow.Huang/My documents/Desktop/Output/*.png\" \"**/{images,images/*}/*.{jpg,png,gif,svg}\" -e \"**/{images,images/*}/sprite/*.{jpg,png,gif,svg}\" -- echo {{changed}}",
		"watch": "concurrently \"npm:watch:*:nodemon\"",
		"min:css": "node task/cssmin.js",
		"min:image": "node task/imagemin.js",
		"min:css+image": "del /s npm-debug.log.* && npm run deploy:css && npm run deploy:image"
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/isobartw-dev/CampaignBuildTool.git"
	},
	"author": "ISOBAR_TW-frontend",
	"bugs": {
		"url": "https://github.com/isobartw-dev/CampaignBuildTool.git/issues"
	},
	"homepage": "https://github.com/isobartw-dev/CampaignBuildTool.git#readme"
}
