{
	"name": "CampaignBuildTool",
	"version": "1.0.0",
	"description": "ISOBAR TW frontend campaign build tool",
	"devDependencies": {
		"autoprefixer": "latest",
		"browser-sync": "^2.12.5",
		"css-mqpacker": "latest",
		"cssnano": "latest",
		"imagemin": "5.2.2",
		"imagemin-jpeg-recompress": "5.1.0",
		"imagemin-pngquant": "5.0.0",
		"imagemin-gifsicle": "5.1.0",
		"imagemin-svgo": "5.1.0",
		"images": "3.0.0",
		"ngrok": "^2.1.7",
		"nodemon": "1.11.0",
		"parallelshell": "latest",
		"postcss": "5.1",
		"postcss-clean": "^1.0.1",
		"postcss-sprites": "3.1.2",
		"precss": "1.4.0",
		"postcss-scss": "0.1.9",
		"tiny-sprintf": "latest",
		"dev-ip": "1.0.1"
	},
	"scripts": {
		"prepublish": "node task/init.js",
		"runIIS": "node task/runIIS.js",
		"watch:css": "nodemon -q -w css -w sass -e css -i **/css/style.css -C --exec node task/watch-postcss.js",
		"watch:image": "nodemon -q -w \"E:/你的名字/My documents/Desktop/Output/**/*\" -e png -i '*.jpg' --exec node task/imagefolder.js",
		"watch": "parallelshell \"npm run watch:image\" \"npm run watch:css\"",
		"deploy:css": "node task/deploy-postcss.js",
		"deploy:image": "node task/imagemin.js",
		"deploy": "npm run deploy:css && npm run deploy:image",
		"dev": "parallelshell \"npm run watch\" \"node task/browsersync.js\""
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/isobartw-dev/CampaignBuildTool.git"
	},
	"author": "ISOBAR_TW-frontend",
	"bugs": {
		"url": "https://github.com/isobartw-dev/CampaignBuildTool.git/issues"
	},
	"homepage": "https://github.com/isobartw-dev/CampaignBuildTool.git#readme"
}
